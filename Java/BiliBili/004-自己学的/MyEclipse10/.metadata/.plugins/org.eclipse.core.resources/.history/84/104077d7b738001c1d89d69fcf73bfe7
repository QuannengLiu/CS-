package com.bjpowernote.javase.test002;


//定义一个妻子类
public class Wife{
	
	//定义身份证号
	String id;
	
	//定义姓名
	String name;
	
	//定义出生日期
	String birth;
	
	//定义丈夫;
	Husband hu;
	
	//定义构造函数
	public Wife(){
		
		id = "1234";
		name = "Amy";
		birth = "20011007";
		//hu = new Husband();
		
	}
	
	public Wife(String name_){
		
		name = name_;
		
	}	
	
}