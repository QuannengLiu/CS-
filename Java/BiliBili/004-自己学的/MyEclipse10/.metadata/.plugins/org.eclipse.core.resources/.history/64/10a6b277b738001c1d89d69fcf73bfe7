package com.bjpowernote.javase.test002;

//定义一个丈夫类
public class Husband {
		
	//定义身份证号
	String id;
	
	//定义姓名
	String name;
	
	//定义出生日期
	String birth;
	
	//定义妻子;
	Wife wi;
	
	//定义构造函数
	public Husband(){
		
		id = "1234";
		name = "JACK";
		birth = "20011007";	
		w = new Wife();
		
	}
	
	public Husband(String name_){
		
		name = name_;
		
	}
}

