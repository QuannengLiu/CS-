package com.bjpowernode.javase.test001;

public class PolymorphismTest01 {
	
	public static void main(String args[]){
		
		//向上转换，自动转换，把cat类型转换成Animal类型
		Animal a2 = new Cat();
		
		a2.move();
		
		/*在编译期时a2时Animal类型，没有抓老鼠，需要进行向下强制类型转换
		 * 
		a2.catchMouse();*/
		
		Cat c2 = (Cat) a2;
		
		c2.catchMouse();

		Animal a3 = new Bird();

	}


}
